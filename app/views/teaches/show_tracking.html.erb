<% if @teach.enrollments.only_students.empty? %>
  <div class="alert"><%= t('view.teaches.without_entollments_in_show.html') %></div>
<% else %>
  <table class="table table-striped table-bordered table-hover table-condensed">
    <thead>
      <tr>
        <th><%= t('view.teaches.enrollment_types.student', count: 1) %></th>
        <th><%= t('view.teaches.visited_content') %></th>
        <th><%= t('view.teaches.questions_answered') %></th>
      </tr>
    </thead>
    <tbody>
      <% @teach.enrollments.only_students.each do |enrollment| %>
        <tr>
          <td><%= enrollment.user %></td>
          <td><%= show_teach_visit_progress_to(@teach, enrollment.user) %></td>
          <td><%= show_teach_survey_progress_to(@teach, enrollment.user) %></td>
        </tr>
      <% end %>
     </tbody>
  </table>

  <p>
    <%= link_to(
      t('label.download_with_format.html', file_format: 'CSV'),
      show_tracking_teach_path(@teach, format: 'csv'), class: 'btn'
    ) %>
  </p>
<% end %>
