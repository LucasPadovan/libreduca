<%= simple_form_for([@content, @homework, @presentation]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :file %>
  </div>

  <div class="form-actions">
    <div class="pull-right"><%= yield :form_actions %></div>
    <%= f.button :submit, class: 'btn-primary' %>
  </div>
<% end %>
