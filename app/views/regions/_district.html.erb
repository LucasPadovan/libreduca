<div class="row-fluid district">
  <div class="span10">
    <%= f.input :name, input_html: { autofocus: true, maxlength: 255, class: 'span10' }, label: false %>
  </div>
  <div class="span2 nested-actions">
    <%= f.input :id, as: :hidden %>
    <%= f.input :lock_version, as: :hidden %>
    <%= link_to_remove_nested_item(f) %>
  </div>
</div>